    "@ratecreator/auth": "1.0.0",
    "@ratecreator/types": "1.0.0",
    "@ratecreator/hooks": "1.0.0",
    "algoliasearch": "^5.20.0",
    "esbuild": "^0.24.0",
    "kafkajs": "^2.2.4"
  },
  "devDependencies": {
    "@types/node": "^20.11.17",
    "tsx": "^4.7.1",
    "typescript": "^5.7.2"
