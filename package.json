{
  "private": true,
  "scripts": {
    "build": "turbo build",
    "start": "turbo start",
    "dev": "turbo dev",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "lint": "turbo lint",
    "type-check": "turbo type-check",
    "clean": "turbo clean",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "prepare": "husky",
    "changeset": "changeset",
    "version-packages": "changeset version",
    "release": "changeset publish",
    "prisma-generate": "npx dotenv -e .env -- npx prisma generate --schema=packages/db/prisma/schema.prisma",
    "category-dev": "yarn workspace @ratecreator/category dev",
    "category-build": "yarn workspace @ratecreator/category build",
    "category-start": "yarn workspace @ratecreator/category start",
    "npm-clean": "find . -name \"node_modules\" -type d -prune -exec rm -rf '{}' +"
  },
  "devDependencies": {
    "@ratecreator/typescript-config": "*",
    "dotenv-cli": "^7.4.2",
    "esbuild": "^0.24.0",
    "eslint": "^8.57.0",
    "husky": "^9.0.11",
    "prettier": "^3.2.5",
    "prettier-plugin-tailwindcss": "^0.5.11",
    "tsx": "^4.19.1",
    "turbo": "^2.0.14",
    "vitest": "^1.6.0"
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "yarn@1.22.22",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "dependencies": {
    "@changesets/cli": "^2.27.6",
    "@clerk/clerk-sdk-node": "^5.0.35",
    "@clerk/nextjs": "^5.4.1",
    "@clerk/themes": "^2.1.27",
    "@icons-pack/react-simple-icons": "^10.0.0",
    "@radix-ui/react-popover": "^1.1.1",
    "@types/lodash": "^4.17.7",
    "@types/node": "^22.4.2",
    "@types/supertest": "^6.0.2",
    "algoliasearch": "v5",
    "axios": "^1.7.7",
    "framer-motion": "^11.3.31",
    "ioredis": "^5.4.1",
    "mongodb": "^6.8.0",
    "posthog-js": "^1.164.1",
    "react-icons": "^5.3.0",
    "react-instantsearch-dom": "^6.40.4",
    "react-social-icons": "^6.18.0",
    "supertest": "^7.0.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.4"
  },
  "name": "ratecreator"
}
