{
  "private": true,
  "scripts": {
    "build": "yarn workspace @ratecreator/ui build && yarn prisma-generate && turbo build",
    "start": "turbo start",
    "dev": "turbo dev",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "lint": "turbo lint",
    "type-check": "turbo type-check",
    "clean": "turbo clean",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "prepare": "husky",
    "changeset": "changeset",
    "version-packages": "changeset version",
    "release": "changeset publish",
    "prisma-generate": "dotenv -e .env yarn prisma generate --schema=packages/db/prisma/schema.prisma",
    "npm-clean": "find . -name \"node_modules\" -type d -prune -exec rm -rf '{}' +",
    "dist-clean": "find . -name \"dist\" -type d -prune -exec rm -rf '{}' +",
    "full-clean": "yarn npm-clean && yarn dist-clean",
    "generate-sitemaps": "yarn tsx scripts/generate-sitemaps.ts"
  },
  "devDependencies": {
    "@ratecreator/typescript-config": "*",
    "@types/algoliasearch": "^4.0.4",
    "@vitest/coverage-v8": "3.0.5",
    "dotenv-cli": "^7.4.4",
    "esbuild": "^0.24.2",
    "eslint": "^8.57.1",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "prettier": "^3.7.4",
    "prettier-plugin-tailwindcss": "^0.5.14",
    "tsx": "^4.19.2",
    "turbo": "^2.7.4",
    "vitest": "^3.0.5"
  },
  "lint-staged": {
    "apps/**/*.{js,jsx,ts,tsx}": [
      "prettier --write"
    ],
    "packages/**/*.{js,jsx,ts,tsx}": [
      "prettier --write"
    ],
    "*.{json,css,scss,md}": [
      "prettier --write"
    ]
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "yarn@1.22.22",
  "workspaces": [
    "apps/*",
    "apps/services/*",
    "apps/webhooks/*",
    "apps/consumers/*",
    "packages/*"
  ],
  "dependencies": {
    "@aws-sdk/client-s3": "^3.758.0",
    "@aws-sdk/s3-request-presigner": "^3.758.0",
    "@changesets/cli": "^2.27.12",
    "@clerk/express": "^1.3.48",
    "@clerk/nextjs": "^5.7.5",
    "@clerk/themes": "^2.2.18",
    "@hookform/resolvers": "^4.1.2",
    "@icons-pack/react-simple-icons": "^10.2.0",
    "@radix-ui/react-popover": "^1.1.6",
    "@react-email/components": "^0.0.36",
    "@types/fuzzy-search": "^2.1.5",
    "@types/lodash": "^4.17.15",
    "@types/node": "^22.13.2",
    "@types/supertest": "^6.0.2",
    "algoliasearch": "^5.20.2",
    "axios": "^1.8.2",
    "cheerio": "^1.0.0",
    "framer-motion": "^11.18.2",
    "fuzzy-search": "^3.2.1",
    "ioredis": "^5.5.0",
    "mongodb": "^6.13.0",
    "next-sitemap": "^4.2.3",
    "posthog-js": "^1.335.2",
    "quill": "^2.0.3",
    "react-icons": "^5.4.0",
    "react-instantsearch": "^7.15.3",
    "react-instantsearch-dom": "^6.40.4",
    "react-markdown": "^9.0.3",
    "react-quill": "^2.0.0",
    "react-social-icons": "^6.22.0",
    "react-textarea-autosize": "^8.5.7",
    "react-tweet": "^3.2.1",
    "rehype-raw": "^7.0.0",
    "resend": "^4.4.1",
    "sharp": "^0.33.5",
    "supertest": "^7.0.0",
    "svix": "^1.45.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2",
    "uuid": "^11.1.0",
    "zod": "^3.24.2"
  },
  "name": "ratecreator"
}
