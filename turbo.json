{
  "$schema": "https://turborepo.dev/schema.json",
  "ui": "tui",
  "concurrency": "19",
  "globalDependencies": ["**/.env.*local"],
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", ".next/**", "!.next/cache/**"]
    },
    "@ratecreator/auth#build": {
      "dependsOn": ["^build"],
      "outputs": []
    },
    "lint": {},
    "type-check": {},
    "dev": {
      "cache": false,
      "persistent": true
    },
    "start": {
      "dependsOn": ["build"]
    },
    "clean": {
      "cache": false
    }
  }
}
